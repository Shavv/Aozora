var i, player, time_left;
//-1 indicates unknown time left, there aren't even the minimum amount of players
//required for a battle
if(gms_vs_time() == -1)
{
    time_left = "We're waiting for at least 2 people.";
}else{
    time_left = string(gms_vs_time());
}

//I've confiured the game settings on the site in such a way, that you'll always wait 20 seconds;
//If you've then got less players than the preferred amount, you'll wait an other 20 seconds (up to a total of 40);
//But, if we have enough players at the 20-second-mark, we'll just start right away.

draw_text(320, y + sprite_height + 10, "Players online: "+string(gms_other_count() + 1)+" / 2#Time Left: " + time_left + "s");
for(i = 0; i &lt; gms_other_count(); i += 1)
{
    player = gms_other_find(i);
    draw_text(320, y + sprite_height + 60 + i * 14, gms_other_get_string(player, "name"));
}
</string>
